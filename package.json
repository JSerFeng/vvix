{
  "name": "vvix",
  "version": "0.1.4",
  "description": "a lite vue-like reactive jsx framework",
  "main": "./build/bundle.cjs.js",
  "module": "./build/bundle.esm.js",
  "scripts": {
    "serve": "set NODE_ENV=dev && rollup -c ./rollup.config.serve.js -w",
    "build": "set NODE_ENV=prod && rollup -c ./rollup.config.prod.js",
    "release": "rollup -c ./rollup.config.prod.js && npm publish",
    "test": "jest",
    "coverage": "jest --coverage",
    "verbose": "npx jest --verbose"
  },
  "author": "JSerFeng",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@types/jest": "^26.0.20",
    "jest": "^26.6.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^26.5.3",
    "@babel/preset-react": "^7.12.13",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.2.3"
  }
}
